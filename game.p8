-- game
ply_vert_spr=1
ply_hori_spr=2
ply={
 -- tile coords
	cx=7,
	cy=7,
	
	-- tile fraction
	rx=0,
	ry=0,
	
	-- velocity
	dx=0,
	dy=0,
	
	is_vert=true,
	is_flip=false,
	
	shoot_dir={
		x=0,
		y=-1,
	}
}
ply_spd=(100/60)/8 -- px/sec
shoot_spd_mult=0.85
shoot_time=8 -- frame delay
shoot_timer=0

bullets={}
spr_bullet=3
spd_bullet=120/60 -- px/sec

cell_x=3
cell_y=3

scroll_x=64
scroll_y=64

function ply_shoot()
	dir=ply.shoot_dir
	bullet={
		x=pixel_x(ply)+dir.x*8,
		y=pixel_y(ply)+dir.y*8,
		spd={
			x=dir.x*spd_bullet,
			y=dir.y*spd_bullet,
		},
		update=b_linear,
	}
	add(bullets,bullet)
end

function pixel_x(ent)
 return ent.cx*8+ent.rx*8
end

function pixel_y(ent)
 return ent.cy*8+ent.ry*8
end

function move_ply()
 --
 -- horizontal
 ply.rx+=ply.dx
 local cx,cy=room.t.coord(ply.cx+1,ply.cy)
 if fget(mget(cx,cy),7) and ply.rx>0 then
  ply.dx=0
  ply.rx=0
 end
 local cx,cy=room.t.coord(ply.cx-1,ply.cy)
 if fget(mget(cx,cy),7) and ply.rx<0 then
  ply.dx=0
  ply.rx=0
 end
	while ply.rx>1 do
	 ply.rx-=1
	 ply.cx+=1
	end
	while ply.rx<0 do
	 ply.rx+=1
	 ply.cx-=1
	end
	
	--
	-- vertical
	ply.ry+=ply.dy
 local cx,cy=room.t.coord(ply.cx,ply.cy+1)
	if fget(mget(cx,cy),7) and ply.ry>0 then
  ply.dy=0
  ply.ry=0
 end
 local cx,cy=room.t.coord(ply.cx,ply.cy-1)
 if fget(mget(cx,cy),7) and ply.ry<0 then
  ply.dy=0
  ply.ry=0
 end
	while ply.ry>1 do
	 ply.ry-=1
	 ply.cy+=1
	end
	while ply.ry<0 do
	 ply.ry+=1
	 ply.cy-=1
	end
end

function _init()
	poke(0x5f2d,0x1)
	palt(6, true)
	palt(0, false)

 local plan=floor_plans[1]
 floor=floor_from_plan(plan)
 room=floor[cell_x][cell_y]
end

function _update60()
	-- player wish direction
	move_wish=move_axes()
	if not is_zero(move_wish) then
		normalize(move_wish)

		-- set state for player sprite
		ply.is_vert=move_wish.x==0
		if ply.is_vert then
			ply.is_flip=move_wish.y>0
		else
			ply.is_flip=move_wish.x>0
		end
	end
	
	shoot_wish=shoot_axes()
	ply.shoot=not is_zero(shoot_wish)
	if ply.shoot then
		normalize(shoot_wish)
		ply.shoot_dir=shoot_wish

		-- shooting overrides the
		-- player's sprite dir/flip
		ply.is_vert=shoot_wish.x==0
		if ply.is_vert then
			ply.is_flip=shoot_wish.y>0
		else
			ply.is_flip=shoot_wish.x>0
		end
	end
	
	if shoot_timer>0 then
		shoot_timer-=1
	elseif ply.shoot then
 	ply_shoot()
 	shoot_timer=shoot_time
 end
 
 remove_bullets={}
 for _,bullet in ipairs(bullets) do
	 -- update each bullet
	 bullet.update(bullet)

	 -- remove bullets which are
	 -- beyond the bounds of the
	 -- play field
 	if bullet.x<-500 or
 	 	bullet.x>500 or
 	 	bullet.y<-500 or
 	 	bullet.y>500 then
 		add(remove_bullets,bullet)
 	end
 end
 
 for _,bullet in ipairs(remove_bullets) do
 	del(bullets,bullet)
 end
 
 use_spd=ply_spd
 if ply.shoot then
 	use_spd*=shoot_spd_mult
 end
	
	-- updating player spd from
	-- player wish direction
	ply.dx=move_wish.x*use_spd
	ply.dy=move_wish.y*use_spd
	
	-- updating player pos from vel
	move_ply()
	
	--room.t.collide(room.t, ply)
	
	scroll_x=pixel_x(ply)
	scroll_y=pixel_y(ply)
	clamp_scroll_to_room()
end

function clamp_scroll_to_room()
 -- clamp camera to room
 local left=64
 local top=64
 local right=room.t.w*128-64
 local bottom=room.t.h*128-64

 if scroll_x<left then
 	scroll_x=left
 end
 if scroll_x>right then
  scroll_x=right
 end
 if scroll_y<top then
  scroll_y=top
 end
 if scroll_y>bottom then
  scroll_y=bottom
 end
end

function _draw()
	cls(0)
	camera(scroll_x-64, scroll_y-64)
	
	room.t.draw()

	if ply.is_vert then
		spr(
			ply_vert_spr,
			pixel_x(ply), pixel_y(ply), 1, 1,
			false, ply.is_flip)
	else
		spr(
			ply_hori_spr,
			pixel_x(ply), pixel_y(ply), 1, 1,
			ply.is_flip, false)
	end
	for _,bullet in ipairs(bullets) do
		spr(spr_bullet,bullet.x,bullet.y)
	end
	
	for _,link in ipairs(room.links) do
	 local ix=link.dcx*128
	 local iy=link.dcy*128
	 local x1,y1,x2,y2=0,0,0,0
	 local spr1=19
	 local flip_x,flip_y=false,false
	 if link.dir==d_up then
	  x1=56
	  x2=64
	 elseif link.dir==d_down then
	  x1=56
	  x2=64
	  y1=120
	  y2=120
	  flip_y=true
	 elseif link.dir==d_left then
	  y1=64
	  y2=56
	  spr1=21
	 elseif link.dir==d_right then
	  y1=64
	  y2=56
	  x1=120
	  x2=120
	  spr1=21
	  flip_x=true
	 end
	 spr(spr1,ix+x1,iy+y1,1,1,flip_x,flip_y)
	 spr(spr1+1,ix+x2,iy+y2,1,1,flip_x,flip_y)
	end
end
